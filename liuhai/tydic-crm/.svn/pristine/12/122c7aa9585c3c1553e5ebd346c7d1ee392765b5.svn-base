// 显示数据字典树形列表
refleshTree();
function refleshTree() {
	$("#org_tree").html("");
	layui.use('tree', function() {
		$.ajax({
			type     : 'GET',
			url      : '../../base/list',
			dataType : 'json',
			async    : true,
			success  : function(result) {
				layui.tree({
				  	elem: '#org_tree' //传入元素选择器
				  	,click: function(item) {
				  		treeId = item.id;
				  		level = item.level;
				  		pcode = item.pcode;
				  		code = item.alias;
				  		treeName = item.name;
				  		if(level == 1){
				  			$("#midLevel").val(1);
				  		}
				  		if(level == 2){
				  			$("#midLevel").val(2);
				  		}
				  		if (level == 3)
				  		{
				  			$("#midLevel").val(3);
				  			showCust(treeId);
				  			// 点击菜单选项-显示项目信息
				  			showproject(treeId);
				  			// 点击菜单选项-显示联系人信息
				  			showLinkman(treeId, proId);
				  			// 点击菜单选项-显示会议纪要列表数据
				  			showMeetingList(treeId, proId);
				  		}
				  	},nodes: result
				});
			}
		});
	});
}

// 增加树节点
function addTree()
{
	if (treeId == 0) {
		alert('请选取一个节点');
	} else if(level > 0) {
		if (level == 1)
		{
			showAddProvDiv();
		}
		else if (level == 2)
		{
			showAddCustDiv(code,pcode);
		}
		else if(level == 3)
		{
			alert('该层节点无添加操作');
		}
	} else {
		alert('请选取一个节点');
	}
}

// 删除树节点
function delTree()
{
	if (treeId == 0) {
		alert('请选择一个节点');
	}
	if (treeId > 0 && level > 0) {
		layer.confirm(treeName + ',确认删除?',{
			title:'提示:可删除区域/省份地市/客户'
			,btn:['确认', '取消']
			,yes:function(index,layerno)
			{
				$.ajax({
					type     : "GET",
					url      : "../../base/delTree/" + treeId + "/" + level,
					dataType : "json",
					success  : function(result) {
						if (result) {
							refleshTree();
							showCust(1);
						} else {
							alert("删除失败，请重试");
						}
					}, 
					error    : function(err) {
						alert(err);
					}
				});
				
				layer.close(index);
			}
		});
	} else {
		alert('请选择一个节点');
	}
}

