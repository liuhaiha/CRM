// 获取所属区域下拉选择框数据
function getRegion() {
	$.ajax({
		type:"GET",
		async: true,
		dataType: "json",
		url: "../../base/getRegion",
		success: function(result) {
			var html = "<option value=''>-请选择-</option>";
			if (result != null) {
				$.each(result, function(i, unit) {
					html += "<option value='" + unit.code +"'>" + unit.name +"</option>";
				});
			}
			$(".region").html(html);
		}
	});
}

